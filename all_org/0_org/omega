FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 3; // 15; 190;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    oversetPatch
    {
        type            overset;
        value           $internalField;
    }
    wing
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    sides
    {
        type            zeroGradient;
    }
}

